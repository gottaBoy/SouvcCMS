<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 数据库驱动包位置 -->
	<classPathEntry location="mysql-connector-java-3.1.13-bin.jar" /> 
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator><property name="suppressAllComments" value="true" /></commentGenerator>
		<!-- mysql数据库链接URL、用户名、密码 -->
		 <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3307/souvccms" userId="root" password="usbw"></jdbcConnection>
		<javaTypeResolver><property name="forceBigDecimals" value="false" /></javaTypeResolver>
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.souvc.cms.entity" targetProject="src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 要生成那些表(更改tableName和domainObjectName就可以) -->
